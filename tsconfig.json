{
    "compilerOptions": {
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "resolveJsonModule": true,
        "jsx": "react",
        "lib": [
            "es6"
        ],
        "moduleResolution": "node",
        "noEmit": true,
        "strict": false,
        "target": "esnext",
        "baseUrl": "./src",
        "paths": {
            "@components/*": [
                "components/*"
            ],
            "@utils/*": [
                "utils/*"
            ],
            "@constant/*": [
                "constant/*"
            ],
            "@stacks/*": [
                "stacks/*"
            ],
             "@features/*": [
                "features/*"
            ],
            "@providers/*": [
                "providers/*"
            ],
            "@hooks/*": [
                "hooks/*"
            ],
            "@assets/*": [
                "assets/*"
            ],
        }
    },
    "exclude": [
        "node_modules",
        "babel.config.js",
        "metro.config.js",
        "jest.config.js"
    ]
}